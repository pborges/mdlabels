{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "check": "deno check src/**/*.ts src/**/*.tsx"
  },
  "imports": {
    "solid-js": "npm:solid-js@^1.9.10",
    "solid-js/": "npm:solid-js@^1.9.10/",
    "jspdf": "npm:jspdf@^4.0.0",
    "vite": "npm:vite@^7.2.4",
    "vite-plugin-solid": "npm:vite-plugin-solid@^2.11.10",
    "tailwindcss": "npm:tailwindcss@^3.4.19",
    "autoprefixer": "npm:autoprefixer@^10.4.23",
    "postcss": "npm:postcss@^8.5.6",
    "typescript": "npm:typescript@~5.9.3",
    "@types/node": "npm:@types/node@^24.10.1"
  },
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "nodeModulesDir": "auto"
}
